<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:visibility="gone"
        android:id="@+id/session_detail_emoji_fl"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:orientation="vertical"
            >
        <chat.icloudsoft.userwebchatlib.ui.adapter.JazzyViewPager
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:id="@+id/session_detail_emoji_viewpager"
            >
        </chat.icloudsoft.userwebchatlib.ui.adapter.JazzyViewPager>
            <chat.icloudsoft.userwebchatlib.widget.view.CirclePageIndicator
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:id="@+id/session_detail_emoji_indicator"
                />
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
